{
    "proxyHost": "0.0.0.0",
    "proxyPort": 8081,
    "variables": {
        "protocol": "https",
        "branchRoot": "c:/dev/depot/main",
        "env": "slce002",
        "domain" : "www.${env}.com"
    },
    "answers" : {

    },
    "mappings": {

    },
    "handlers" : {
        "https://sell.slce002.co.uk/sfr*" : {
            "className" : "com.kingfisher.proxy.custom.ReplaceHandler",
            "parameters": {
                " var defaultLocale = 'en-gb'" : " var defaultLocale = 'de-de'"
            }
        },
        "https://sell.slce002.com/sfr*" : {
            "className" : "com.kingfisher.proxy.custom.ReplaceHandler",
            "parameters": {
                " var defaultLocale = 'en-us'" : " var defaultLocale = 'de-de'"
            }
        },
        "https://sell.slce002.de/sfr*" : {
            "className" : "com.kingfisher.proxy.custom.ReplaceHandler",
            "parameters": {
                " var defaultLocale = 'de-de'" : " var defaultLocale = 'en-gb'"
            }
        }
    },
    "chainedProxies": [
        {
            "host": "slc-entbc-001",
            "port": 80,
            "disabled" : true,
            "bypassHosts": [
                "*srwd*",
                "*slcq*"
            ]
        }
    ]
}
