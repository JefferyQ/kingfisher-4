{
    "proxyHost": "0.0.0.0",
    "proxyPort": 8081,
    "variables": {
        "protocol": "https",
        "branchRoot": "c:/dev/depot/main",
        "env": "stubhub",
        "domain" : "www.${env}.com"
    },
    "answers" : {
        "https://m.stubhub.com/shape/accountmanagement/orders/v1/buyer/*" : 
        {
	        "version":"1.1",
	        "statusCode":404,
	        "delayMS": 200,
	        "headers":{
	        	"Keep-Alive":"timeout=5, max=100",
	        	"Connection":"Keep-Alive",
	        	"activityID":"activityID1",
	        	"com-stubhub-dye":"com-stubhub-dye1"
	        	},
	        "body":"<html>ERROR</html>"
        },
        "https://m.stubhub.com/shape/accountmanagement/listings/v1/seller/*" : 
        {
	        "version":"1.1",
	        "statusCode":404,
	        "delayMS": 200,
	        "headers":{
	        	"Keep-Alive":"timeout=5, max=100",
	        	"Connection":"Keep-Alive",
	        	"activityID":"activityID1",
	        	"com-stubhub-dye":"com-stubhub-dye1"
	        	},
	        "body":"<html>ERROR</html>"
        }
    },
    "mappings": {
        "http://cache*.stubhubstatic.com/resources/mojito/js/feature/bundle-ep-bp-seatmapPagi-*.*.0-SNAPSHOT.js" : "e:\\bundle.js",
        "http://www.${env}.com/<x>/<y>/event/<id>/" : "http://www.${env}.com/eventpage-seopoc/?event_id=<id>",
        "http://www.${env}.com/<x>/performer/<id>/<p>" : "http://www.${env}.com/performer-seopoc/?performer_id=<id>"
    },
    "handlers" : {
        "https://m.slcq001.com/shape/accountmanagement/*" : {
           "className" : "com.stubhub.proxy.custom.PseudolocalizationHandler",
           "parameters": {
                /* regex for gson value */
		        "targetTextPatternInRegx": "(\\\"[^\\\"]*?\\s+?[^\\\"]+?\\\"|\\\"[A-Z][a-z]+?\\\")"
		    }
        },
        "http://slcq001mci001.slcq001.com:8080/solr/*" : {
           "className" : "com.stubhub.proxy.custom.cacher.MemoryCacher",
           "parameters": {
               "max" : 100,
               "timeToLiveSecond" : "60"
            }
        }
    },
    "chainedProxies": [
        {
            "host": "slc-entbc-001",
            "port": 80,
            "disabled" : true,
            "bypassHosts": [
                "*srwd*",
                "*slcq*",
                "*stubhub*"
            ]
        }
    ]
}
