{
    "proxyPort": 8081,
    "variables": {
        "protocol": "https",
        "branchRoot": "c:/dev/depot/main",
        "env": "stubhub",
        "domain" : "www.${env}.com"
    },
    "answers" : {
        "http://abc.abc" : 
        {
	        "version":"1.1",
	        "statusCode":200,
	        "delayMS": 1000,
	        "headers":{
	        	"Keep-Alive":"timeout=5, max=100",
	        	"Connection":"Keep-Alive"
	        	},
	        "body":"<html>ABCD</html>"
        },
        "https://abcd.abcd" : 
        {
	        "version":"1.1",
	        "statusCode":200,
	        "delayMS": 2000,
	        "headers":{
	        	"Keep-Alive":"timeout=5, max=100",
	        	"Connection":"Keep-Alive"
	        	},
	        "body":"<html>ABCDEFG</html>"
        }
    },
    "mappings": {
        "http://www.${env}.com/<x>/<y>/event/<id>/" : "http://www.${env}.com/eventpage-seopoc/?event_id=<id>",
        "http://www.${env}.com/<x>/performer/<id>/<p>" : "http://www.${env}.com/performer-seopoc/?performer_id=<id>"
    },
    "chainedProxies": [
        {
            "host": "slc-entbc-001",
            "port": 80,
            "bypassHosts": [
                "*srwd*",
                "*stubhub*"
            ]
        }
    ]
}
